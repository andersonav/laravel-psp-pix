{
    "name": "andersonav/laravel-psp-pix",
    "description": "Integration PIX with Laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "Alves",
        "pix",
        "bacen",
        "pix-api"
    ],
    "require": {
        "php": "^8.0|^8.2|^8.3|^8.4",
        "mpdf/qrcode": "^1.2",
        "symfony/psr-http-message-bridge": "^7.1"
    },
    "require-dev": {
        "orchestra/testbench": "^9.3",
        "phpunit/phpunit": "^11.3",
        "guzzlehttp/guzzle": "^7.9",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "andersonav",
            "email": "andersonalves.dev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alves\\Pix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alves\\Pix\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alves\\Pix\\Providers\\PixServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}